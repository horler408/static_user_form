<% if(typeof errors != 'undefined'){ %> 
    <% errors.forEach(function(error) { %>
    <div class="errors_msg">
      <%= error.msg %>
    </div>
    <% }); %> 
<% } %> 

<% if(success_msg != ''){ %>
    <div class="success_msg"><%= success_msg %></div>
<% } %> 

<% if(error_msg != ''){ %>
    <div class="error_msg""><%= error_msg %></div>
<% } %> 

<% if(error != ''){ %>
    <div class="error_alert""><%= error %></div>
<% } %>
    